Init.        Main ::= Type Type Integer Double Char String Ident MyId;

TypeType.    Type ::= "Type"    ;  -- #235 conflict with non-terminal Type
TypeInteger. Type ::= "Integer" ;  -- #235 conflict with token type Integer
TypeDouble.  Type ::= "Double"  ;  -- etc.
